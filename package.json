{
  "name": "vicvarros",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/Release/server",
    "watch": "DEBUG=server nodemon -L ./server --exec babel-node --presets airbnb",
    "clean": "rimraf build",
    "compile": "babel ./ --presets airbnb --out-dir build/Release --ignore ./node_modules,./LICENSE,./README.md,./package-lock.json --copy-files",
    "build": "npm run clean && npm run compile",
    "predeploy": "npm run build && cd ./build/Release",
    "test": "mocha --require babel-core/register"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "8.5.0"
  }
}