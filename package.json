{
  "name": "vicvarros",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server",
    "watch": "DEBUG=debug:* nodemon -L ./server --exec babel-node --presets airbnb",
    "clean": "rimraf ./build",
    "compile": "babel ./ --presets airbnb --out-dir ./build/Release --ignore ./node_modules,./LICENSE,./README.md,./package-lock.json,./tests --copy-files",
    "build": "npm run clean && npm run compile",
    "test": "mocha ./tests --require babel-core/register"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-routes-versioning": "^1.0.1",
    "express-validator": "^4.3.0",
    "helmet": "^3.10.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-airbnb": "^2.4.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "8.9.4"
  }
}